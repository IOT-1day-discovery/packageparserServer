{
      "plugins": [
              "plugins/markdown",
              "plugins/summarize"
      ],
      "source": {
              "include": ["./src/", "README.md"]
      },
      "opts": {
              "destination": "./output/Docs",
              "recurse": true,
              "template": "node_modules/docdash",
              "verbose":true
      },
      "templates": {
          "default": {
              "layoutFile": "./jsdoc/template.tmpl"
          }
      }
}
